{
    "name": "jacmoe/bugitor",
    "description": "A Yii-powered issue tracker",
    "keywords": ["yii2", "tracker"],
    "type": "application",
    "license": "MIT",
    "minimum-stability": "dev",
    "require": {
        "php": ">=5.4.0",
        "cebe/assetfree-yii2": "~2.0.0",
        "2amigos/yii2-usuario": "~1.0"
    },
    "require-dev": {
        "codeception/base": "^2.2.3",
        "codeception/verify": "~0.3.1",
        "codeception/specify": "~0.4.3"
    },
    "scripts": {
        "post-create-project-cmd": [
            "yii\\composer\\Installer::postCreateProject"
        ]
    },
    "extra": {
        "yii\\composer\\Installer::postCreateProject": {
            "setPermission": [
                {
                    "api/runtime": "0777",
                    "api/yii": "0755"
                }
            ]
        }
    }
}
